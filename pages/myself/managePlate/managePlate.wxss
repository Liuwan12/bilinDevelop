.myFollowModel_info{
  position: relative;
  width: 100%;
  height: 90rpx;
  border-bottom: 1px solid #D3D3D3;
}
.myFollowModel_info text{
  position: absolute;
  width: 500rpx;
  font-size: 28rpx;
  color: #2B2B2B;
  line-height: 90rpx;
  left: 50rpx; 
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.giveup_follow{
  position:absolute;
  width:168rpx;
  height:40rpx;
  border:1px solid #FF5555;
  border-radius:8rpx;
  font-size:26rpx;
  color:#FF5555;
  line-height:40rpx;
  text-align:center;
  top:25rpx;
  right:48rpx;
}
